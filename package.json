{
  "name": "@nekofar/lilnouns",
  "version": "1.0.0-alpha.15",
  "private": false,
  "description": "TypeScript client for interacting with Lil Nouns",
  "keywords": [
    "lilnouns"
  ],
  "homepage": "https://github.com/nekofar/lilnouns#readme",
  "bugs": {
    "url": "https://github.com/nekofar/lilnouns/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nekofar/lilnouns.git"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Milad Nekofar",
    "email": "milad@nekofar.com"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./contracts": {
      "import": "./dist/contracts/index.js",
      "types": "./dist/contracts/index.d.ts"
    },
    "./subgraphs": {
      "import": "./dist/subgraphs/index.js",
      "types": "./dist/subgraphs/index.d.ts"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**",
    "src/**/*.ts",
    "!src/**/*.test.ts",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "prebuild": "is-ci && exit 0 || (pnpm clean && pnpm graphql:generate && pnpm wagmi:generate)",
    "build": "NODE_OPTIONS=\"--max-old-space-size=8192\" tsup",
    "build:assets": "ts-node scripts/build-assets.ts",
    "postbuild": "pnpm run format",
    "clean": "rimraf dist",
    "docs": "NODE_OPTIONS=\"--max-old-space-size=8192\" typedoc",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "graphql:generate": "graphql-codegen --config codegen.config.ts",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "prepublishOnly": "pnpm build",
    "test": "vitest run",
    "wagmi:generate": "wagmi generate"
  },
  "devDependencies": {
    "@0no-co/graphqlsp": "^1.15.0",
    "@biomejs/biome": "^2.2.2",
    "@graphprotocol/graph-ts": "^0.38.1",
    "@graphql-codegen/cli": "^6.0.0",
    "@graphql-codegen/schema-ast": "^5.0.0",
    "@graphql-codegen/typescript-graphql-request": "^6.3.0",
    "@graphql-codegen/typescript-operations": "^5.0.0",
    "@parcel/watcher": "^2.5.1",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@wagmi/cli": "^2.5.1",
    "dotenv": "^17.2.1",
    "graphql-scalars": "^1.24.2",
    "is-ci": "^4.1.0",
    "rimraf": "^6.0.1",
    "terser": "^5.43.1",
    "tsup": "^8.5.0",
    "typedoc": "^0.28.12",
    "typedoc-plugin-markdown": "^4.8.1",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^5.74.4",
    "@wagmi/core": "^2.16.7",
    "graphql": "^16.10.0",
    "graphql-request": "^6.1.0",
    "graphql-tag": "^2.12.6",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "typescript": ">=5.0.0",
    "viem": "^2.27.2",
    "wagmi": "^2.14.16"
  },
  "packageManager": "pnpm@10.9.0",
  "publishConfig": {
    "access": "public"
  }
}
